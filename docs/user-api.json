                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "description": {
                                                    "type": "string"
                                                },
                                                "error_message": {
                                                    "type": "string"
                                                },
                                                "execute_at": {
                                                    "type": "string"
                                                },
                                                "result": {
                                                    "type": "string"
