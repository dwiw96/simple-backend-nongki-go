                                                },
                                                "execute_at": {
                                                    "type": "string"
                                                },
                                                "result": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }, 
                                "examples": {
                                    "duplicated email": {
                                            "description": "email is registered",
                                            "value": {
                                                "description": "email is registered",
                                                "error_message": "Conflict",
                                                "execute_at": "2024/09/22 19:45:18.494",
                                                "result": "failure"
                                            }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
