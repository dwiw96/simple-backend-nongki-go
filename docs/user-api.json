                                    },
                                    "middle_name":{
                                        "type": "string"
                                    },
                                    "last_name":{
                                        "type": "string"
                                    },
                                    "address":{
                                        "type": "string"
                                    },
                                    "gender":{
                                        "type": "string"
                                    },
                                    "married_status":{
                                        "type": "string"
                                    },
                                    "password":{
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "email", "first_name", "last_name", "address", "gender", "married_status", "password"
                                ]
                            },
                            "examples": {
                                "john doe":{
                                    "description": "Sign up user john doe",
